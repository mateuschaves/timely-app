{
  "name": "timely-app",
  "version": "1.0.4",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "deeplink:entry": "node scripts/test-deeplink.js entry",
    "deeplink:exit": "node scripts/test-deeplink.js exit",
    "deeplink:custom": "node scripts/test-deeplink.js custom",
    "deeplink:expo-go": "node scripts/test-deeplink.js expo-go",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=html --coverageReporters=text-summary",
    "test:coverage:ci": "jest --ci --coverage --maxWorkers=2 --coverageReporters=text --coverageReporters=lcov",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "build:dev": "eas build --profile development --platform all",
    "build:preview": "eas build --profile preview --platform all",
    "build:prod": "eas build --profile production --platform all",
    "build:ios": "eas build --profile production --platform ios",
    "build:android": "eas build --profile production --platform android",
    "update:dev": "eas update --branch development --auto",
    "update:preview": "eas update --branch preview --auto",
    "update:prod": "eas update --branch production --auto",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "submit:all": "eas submit --platform all --latest"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@tanstack/react-query": "^5.56.0",
    "axios": "^1.6.5",
    "date-fns": "^4.1.0",
    "expo": "~54.0.0",
    "expo-apple-authentication": "~8.0.7",
    "expo-blur": "~15.0.7",
    "expo-constants": "~18.0.10",
    "expo-file-system": "~19.0.21",
    "expo-haptics": "~15.0.7",
    "expo-insights": "~0.10.8",
    "expo-intent-launcher": "~13.0.8",
    "expo-linking": "~8.0.9",
    "expo-live-activity": "^0.4.2",
    "expo-localization": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-notifications": "^0.32.13",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "^31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.10",
    "i18next": "^25.6.3",
    "react": "19.1.0",
    "react-i18next": "^16.3.5",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-webview": "13.15.0",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@jest/globals": "^30.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "@types/styled-components-react-native": "^5.2.5",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "react-test-renderer": "^19.1.0",
    "reactotron-core-client": "^2.9.9",
    "reactotron-react-native": "^5.1.18",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.3"
  },
  "private": true
}
